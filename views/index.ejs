<% include _header %>
<% if(users.length !== 0) { %>
  <% users.forEach(function(user) { %>
  <div class="row">
    <div class="col-xs-12">
      <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Accounted created: <%= user.createdAt || "" %></h3>
          </div>
          <div class="panel-body">
            <h3><%= user.userName %></h3>
            <p><%= user.bio %></p>
          </div>
      </div>
    </div>
  </div>
  <% }) %>
<% } %>
<% include _footer %>